<div>
  <h2>Day</h2>
    <p>Month: {{this.model.month}}</p>
    <p>Day: {{this.model.day}}</p>
    <p>Year: {{this.model.year}}</p>
  {{#each this.model.events as |item|}}
    {{item.id}}-{{item.title}}-{{item.start}}<br />
  {{/each}}
</div>
  <LinkTo @route="calendar" class="button">CALENDAR</LinkTo>